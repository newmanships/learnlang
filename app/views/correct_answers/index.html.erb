<h1>Listing correct_answers</h1>

<table>
  <thead>
    <tr>
      <th>Question</th>
      <th>Correctanswertext</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @correct_answers.each do |correct_answer| %>
      <tr>
        <td><%= correct_answer.question_id %></td>
        <td><%= correct_answer.correctAnswerText %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>