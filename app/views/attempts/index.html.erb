<h1>Listing attempts</h1>

<table>
  <thead>
    <tr>
      <th>Quiz</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attempts.each do |attempt| %>
      <tr>
        <td><%= attempt.quiz_id %></td>
        <td><%= attempt.user_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
