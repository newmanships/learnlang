<h1>Listing quizzes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Lesson</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quizzes.each do |quiz| %>
      <tr>
        <td><%= quiz.name %></td>
        <td><%= quiz.lesson_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
