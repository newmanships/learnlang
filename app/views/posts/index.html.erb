<h1>Listing posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th>Image</th>
      <th>Lesson</th>
      <th>Alignment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.body %></td>
        <td><%= post.image %></td>
        <td><%= post.lesson_id %></td>
        <td><%= post.alignment %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
