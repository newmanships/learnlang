<h1>Listing sections</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Course</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sections.each do |section| %>
      <tr>
        <td><%= section.title %></td>
        <td><%= section.course_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

